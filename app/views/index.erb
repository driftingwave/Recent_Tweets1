<div class="container">
  <table width='100%'>
    <col width='200px'>
    <col width='760px'>
    <tr>
      <th>Date</th>
      <th>Tweet</th>
    </tr>
  <% @tweets_all_info.each do |tweet| %>
    <tr>
      <td><%= tweet.authored_date.strftime('%m/%d/%Y at %I:%M%p') %></td>
      <td><%= tweet.text %></td>
    </tr>
  <% end %>
  </table>
</div>
